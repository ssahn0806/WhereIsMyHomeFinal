<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssafy.house.model.dao.GugunCodeDAO">
	<select id="selectGugunCodes" parameterType="string" resultType="GugunCode">
		select gugunCode,gugunName from guguncode where gugunCode like concat(LEFT(#{sidoCode},2),'%') order by gugunName
	</select>
</mapper>














